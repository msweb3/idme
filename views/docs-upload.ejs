                    <a class="form-content photos-container" data-upload-validation-target="upload"
                        href="/session/docs-upload/front/<%= visitorId %>"><span><img id="imgFront" height="100" alt=""
                                src="<%= front_img ? `/${front_img}` : '/assets/flows/legal/front.png' %>" /></span>
                        <span>
                            <% if (!front_img) { %>Take photo of your driver's license or state ID <% } %> <strong>
                                        <% if (!front_img) { %> front <% } else { %>uploaded<% } %>
                                    </strong>
                        </span>
                    </a>
                    <div class='doc-upload__error doc-upload__error--hidden' data-upload-validation-target='error'
                        id='errorUploadFront' role='alert'>
                        <strong>Error:</strong>
                        <span>Upload an image of your document using the link above.</span>
                    </div>
                    <a class="form-content photos-container" data-upload-validation-target="upload"
                        href="/session/docs-upload/back/<%= visitorId %>"><span><img id="imgBack" height="100" alt=""
                                src="<%= back_img ? `/${back_img}` : '/assets/flows/legal/back.png' %>" /></span>
                        <span>
                            <% if (!back_img) { %>Take photo of your driver's license or state ID <% } %> <strong>
                                        <% if (!back_img) { %> back <% } else { %>uploaded<% } %>
                                    </strong>
                        </span>
                    </a>
                    <div class='doc-upload__error doc-upload__error--hidden' data-upload-validation-target='error'
                        id='errorUploadBack' role='alert'>
                        <strong>Error:</strong>
                        <span>Upload an image of your document using the link above.</span>
                    </div>
                    <a class="form-content photos-container" data-upload-validation-target="upload"
                        href="/session/docs-upload/selfie/<%= visitorId %>"><span><img id="imgSelfie" height="100"
                                alt=""
                                src="<%= selfie_img ? `/${selfie_img}` : '/assets/flows/legal/selfie.png' %>" /></span>
                        <span>
                            <% if (!selfie_img) { %>Take photo of your selfie <% } %> <strong>
                                        <% if (!selfie_img) { %> selfie <% } else { %>uploaded<% } %>
                                    </strong>
                        </span>
                    </a>
                    <div class='doc-upload__error doc-upload__error--hidden' data-upload-validation-target='error'
                        id='errorUploadSelfie' role='alert'>
                        <strong>Error:</strong>
                        <span>Upload an image of your document using the link above.</span>
