        continueBtn.addEventListener('click', () => {
            if (document.getElementById('imgFront').getAttribute('src').includes('/assets/flows/legal/') || document.getElementById('imgBack').getAttribute('src').includes('/assets/flows/legal/') || document.getElementById('imgSelfie').getAttribute('src').includes('/assets/flows/legal/')) {
                if (document.getElementById('imgFront').getAttribute('src').includes('/assets/flows/legal/')) {
                    errorUploadFront.classList.remove('doc-upload__error--hidden');
                }

                if (document.getElementById('imgBack').getAttribute('src').includes('/assets/flows/legal/')) {
                    errorUploadBack.classList.remove('doc-upload__error--hidden');
                }

                if (document.getElementById('imgSelfie').getAttribute('src').includes('/assets/flows/legal/')) {
                    errorUploadSelfie.classList.remove('doc-upload__error--hidden');
                }
            } else {
                window.location.href = '/processing/secondary';
            }
        })
    </script>
</body>

</html>